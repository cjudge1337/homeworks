<h1> <%= @album.name %> </h1>


<h2>Tracks</h2>
<ul>
  <% @album.tracks.each do |track| %>
    <li> <%= "#{track.name}" %> </li>
  <% end %>
</ul>

<footer>
  <%= link_to "Edit Album Info", edit_album_url %>
  <br>
  <%= link_to "Return to Band List", bands_url(@album.band_id) %>
  <br>
  <%= button_to "Delete Album", album_url(@album), method: :delete %>
</footer>
