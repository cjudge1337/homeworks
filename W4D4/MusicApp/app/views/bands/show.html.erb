<h1> <%= @band.name %> </h1>


<h2>Albums</h2>
<ul>
  <% @band.albums.each do |album| %>
    <li> <%= link_to "#{album.name}", album_url(album.id) %> </li>
  <% end %>
</ul>

<footer>
  <%= link_to "Edit Band Info", edit_band_url %>
  <br>
  <%= link_to "Return to Bands List", bands_url %>
  <br>
  <%= button_to "Delete Band", band_url(@band), method: :delete %>
</footer>
